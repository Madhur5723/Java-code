
public class Manager extends Employee {
	private int bonus;

	public Manager(int empid, String empname, double empsal, int bonus) {
		super(empid, empname, empsal);
		this.bonus = bonus;
	}

	public int getBonus() {
		return bonus;
	}

	public void setBonus(int bonus) {
		this.bonus = bonus;
	}
	
	

}
